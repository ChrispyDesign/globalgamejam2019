			float2 panner5 = ( _Time.y * _Speed + i.uv_texcoord);
			o.Albedo = ( tex2D( _Texture, panner5 ) * _Colour ).rgb;
			float2 uv_TextureSample0 = i.uv_texcoord * _TextureSample0_ST.xy + _TextureSample0_ST.zw;
